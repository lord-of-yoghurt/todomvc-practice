<section class="todoapp">
	<header class="header">
		<h1>Your lists</h1>
		<input class="new-todo" placeholder="Make a new list" autofocus="">
	</header>
	<section class="main">
		<input class="toggle-all" type="checkbox">
		<label for="toggle-all">Mark all as complete</label>
		<ul class="todo-list">
      <% @lists.each do |l| %>
        <li>
          <div class="view">
            <label><%= link_to l.name, l %></label>
          </div>
        </li>
      <% end %>
    </ul>
	</section>
	<footer class="footer">
	</footer>
</section>
